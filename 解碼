#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define c 5000

int main() {
	
	
	char x[c];
	gets(x);
	
	char ans[c] = {};
	int pos = 0;
	
	for(int i = 0; i = 128; i++) {
		
		char a[c] = {};
		int countblank = 0;
		
		for(int j = 0; x[j] != '\0'; j++) {
			
			int b = (x[j] + i + 256) % 128;
			a[i] = b;
			
			if(a[i] >= '0' && a[i] <= '9') 
				countblank++;
		}
		
		if(countblank > pos){
			strcpy(ans, a);
			pos = countblank;
		}
	}
	
	printf("%s", ans);
	
	return 0;
}
